{
  "name": "my-leaderboard-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "forever": "./script/start",
    "stop": "./script/stop",
    "start": "yarn build && nodemon build/index.js --exec 'node -r dotenv/config'",
    "build": "yarn copy-common && tsc",
    "copy-common": "rm -rf ./src/lib && cp -r ../common ./src/lib",
    "lint": "tslint 'src/**/*.ts?(x)'"
  },
  "dependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^12.0.10",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "forever": "^1.0.0",
    "nodemon": "^1.19.1",
    "octokat": "^0.10.0",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.2"
  }
}
